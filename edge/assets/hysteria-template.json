{
  "dns": {
    "final": "proxy-dns",
    "independent_cache": true,
    "rules": [
      {
        "rule_set": "geosite-ir",
        "server": "direct-dns"
      },
      {
        "clash_mode": "Direct",
        "server": "direct-dns"
      },
      {
        "server": "proxy-dns",
        "source_ip_cidr": ["172.19.0.1/30", "fdfe:dcba:9876::1/126"]
      },
      {
        "clash_mode": "Global",
        "server": "proxy-dns"
      }
    ],
    "servers": [
      {
        "detour": "proxy",
        "domain_resolver": "local-dns",
        "server": "8.8.8.8",
        "tag": "proxy-dns",
        "type": "tcp"
      },
      {
        "detour": "direct",
        "server": "8.8.4.4",
        "tag": "local-dns",
        "type": "tcp"
      },
      {
        "detour": "direct",
        "tag": "direct-dns",
        "type": "local"
      }
    ],
    "strategy": "prefer_ipv4"
  },
  "experimental": {
    "cache_file": {
      "enabled": true
    }
  },
  "inbounds": [
    {
      "address": ["172.19.0.1/30", "fdfe:dcba:9876::1/126"],
      "auto_route": true,
      "mtu": 9000,
      "platform": {
        "http_proxy": {
          "enabled": true,
          "server": "127.0.0.1",
          "server_port": 2080
        }
      },
      "stack": "system",
      "tag": "tun-in",
      "type": "tun"
    },
    {
      "listen": "127.0.0.1",
      "listen_port": 2080,
      "tag": "mixed-in",
      "type": "mixed"
    }
  ],
  "log": {
    "level": "error",
    "timestamp": true
  },
  "outbounds": [
    {
      "default": "hysteria",
      "outbounds": ["auto", "direct", "hysteria"],
      "tag": "proxy",
      "type": "selector"
    },
    {
      "interval": "10m0s",
      "outbounds": ["hysteria"],
      "tag": "auto",
      "tolerance": 50,
      "type": "urltest",
      "url": "http://www.gstatic.com/generate_204"
    },
    {
      "domain_strategy": "prefer_ipv4",
      "tag": "direct",
      "type": "direct"
    },
    {
      "obfs": {
        "password": "KevinZakarian",
        "type": "salamander"
      },
      "password": "KevinZakarian",
      "server": "keviny.iserver.store",
      "server_port": 8443,
      "tag": "hysteria",
      "tls": {
        "enabled": true,
        "insecure": true
      },
      "type": "hysteria2"
    }
  ],
  "route": {
    "auto_detect_interface": true,
    "default_domain_resolver": "direct-dns",
    "final": "proxy",
    "override_android_vpn": true,
    "rule_set": [
      {
        "download_detour": "direct",
        "format": "binary",
        "tag": "geosite-ads",
        "type": "remote",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/category-ads-all.srs"
      },
      {
        "download_detour": "direct",
        "format": "binary",
        "tag": "geosite-private",
        "type": "remote",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/private.srs"
      },
      {
        "download_detour": "direct",
        "format": "binary",
        "tag": "geosite-ir",
        "type": "remote",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geosite/category-ir.srs"
      },
      {
        "download_detour": "direct",
        "format": "binary",
        "tag": "geoip-private",
        "type": "remote",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geoip/private.srs"
      },
      {
        "download_detour": "direct",
        "format": "binary",
        "tag": "geoip-ir",
        "type": "remote",
        "url": "https://testingcf.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo/geoip/ir.srs"
      }
    ],
    "rules": [
      {
        "action": "sniff",
        "timeout": "1s"
      },
      {
        "action": "hijack-dns",
        "protocol": "dns"
      },
      {
        "clash_mode": "Direct",
        "outbound": "direct"
      },
      {
        "clash_mode": "Global",
        "outbound": "proxy"
      },
      {
        "outbound": "direct",
        "rule_set": ["geoip-private", "geosite-private", "geosite-ir", "geoip-ir"]
      },
      {
        "outbound": "block",
        "rule_set": "geosite-ads"
      }
    ]
  }
}
